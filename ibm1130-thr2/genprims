# Please see the LICENSE file for the Affero GPL 3.0 license details

do "mapnames"; # to handle the IBM 1130 assembler 5 character label limit

sub genprims {
	my ($def,$var)=@_;
	while (my ($k,$v)=each %$def) {
		next if ($var->{$k});
		$k=maplbl($k,$k);
		print "$k", join("",@$v);
	}
	print "\nrom\n";
}
