bye	: rts
dup	: jsr realdup
drop	: jsr realdrop
key	: jsr realkey
emit	: jsr realemit
neg	: jsr realneg
j	: clc ; .db 0x90
jz	: cmp #0 ; php ; jsr realdrop ; plp ; .db 0xF0
jnz	: cmp #0 ; php ; jsr realdrop ; plp ; .db 0xD0
inc	: clc ; adc #1
dec	: clc ; adc #-1
