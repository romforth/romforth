next	: goto repl
bye	: stopsys
key	: mov a, #0x7f ; mov p, a ; mov a, #'r' ; idxm p, a ; idxm a, p ; pushaf ;
emit	: mov a, #0x7f ; mov p, a ; mov a, #'w' ; idxm p, a ; popaf ; idxm p, a ;
