bye	: .db 0x39
dup	: .db 0x36, 6
drop	: .db 0x37, 6
key	: dup ; .db 0xbd ; .dw realkey
emit	: .db 0xbd ; .dw realemit ; drop
inc	: .db 0xC3,0,1
dec	: .db 0x83,0,1
inv	: .db 0x53, 0x43
neg	: inv ; inc
dip	: .db 0x36, 0x10
nip	: .db 0x37, 0x10
