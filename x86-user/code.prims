define next	jmp inner
define w	%rax
define ip	%rsi

inner	: lodsb ; jmp *w
key	: push w ; push ip ; call getchar ; mov w, %rdi ; pop ip ; pop w ;
emit	: push w ; push ip ; call putchar ; pop ip ; pop w ;
bye	: ret
